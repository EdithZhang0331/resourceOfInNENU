    "prettier": "2.8.4",
    "qrcode": "1.5.1",
    "svgo": "3.0.2",
    "sort-package-json": "2.4.1",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "vue": "3.2.47",
    "vuepress": "2.0.0-beta.61",
    "vuepress-plugin-search-pro": "2.0.0-beta.185",
