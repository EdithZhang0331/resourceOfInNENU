  "type": "module",
  "scripts": {
    "build:site": "vuepress build site",
    "dev:site": "vuepress dev site",
    "generate": "ts-node --esm ./scripts/gen/index.ts",
    "generate:account": "ts-node --esm ./scripts/gen/account.ts",
    "generate:site": "ts-node --esm ./scripts/gen/site.ts",
